{
  "manifest_version": 2,
  "name": "dickdrumpf",
  "version" : "1.0",
  "description": "Replace all images of Donald Trump with dicbutt",
  "background": {
  	"scripts": ["js/background.js"],
	  "persistent" : true
   },
   "content_scripts": [
    	{	
        "matches": ["http://*/*", "https://*/*"],
        "css": ["style/inject.css"],
        "js": ["js/content.js"]
    	}
	],
  "offline_enabled": false,
	"permissions" : ["activeTab",
  "tabs",
  "https://dev.perooz.io/api/*",
  "storage",
  "contextMenus",
  "cookies",
  "background"],
  "icons": {},
  "web_accessible_resources":[]
}